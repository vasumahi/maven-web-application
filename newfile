pipeline{
agent any 
stages{
     stage(test){
         steps{
         echo 'test'
     } 
     }

      stage(build){
      steps{
         echo 'build'
     } 
     }
       stage(deploy){
        input{
message "hai"
ok " branch selected"
parameters{
     choice(name:'Branch', choices:['dev','qa',],description: 'branch')
 }
 }
 steps{
     script{
         echo "deploying to ${branch}"
     }
 }
       steps{
         echo 'deploy'
     } 
     }
}
}

